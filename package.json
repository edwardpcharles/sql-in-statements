{
  "name": "sql-in-statements",
  "displayName": "List to SQL IN() Statement",
  "description": "This extension allows you to take a list of records separated by a line and turn it into a SQL IN() statement.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.72.0"
  },
  "author": {
    "name": "Edward P Charles"
  },
  "publisher": "edwardpcharles",
  "categories": [
    "Formatters"
  ],
  "keywords":[
    "SQL","SQL IN", "IN Statement", "IN", "T-SQL", "IN()"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#383838",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:sql-in-statements.sqlin"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sql-in-statements.sqlin",
        "title": "SQL IN() Statement"
      }
    ]    
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edwardpcharles/sql-in-statements.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
